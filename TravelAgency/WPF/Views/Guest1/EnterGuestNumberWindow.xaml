<Window x:Class="SOSTeam.TravelAgency.WPF.Views.EnterGuestNumberWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SOSTeam.TravelAgency.WPF.Views"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="EnterGuestNumber" Height="450" Width="450">
    <Grid>
        <Label Content="Unesite broj gostiju ove rezervacije :" FontSize="15" FontWeight="Bold" Margin="91,163,41,204"></Label>
        <TextBox x:Name="GuestNumber" Margin="162,217,162,162"/>
        <Button Content="Rezerviši" Command="{Binding finishReserveCommand}" Margin="179,297,179,86" FontSize="14"/>
    </Grid>
</Window>
