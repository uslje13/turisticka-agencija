<Page x:Class="SOSTeam.TravelAgency.WPF.Views.Owner.PDFReportPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:pdfViewer="clr-namespace:Syncfusion.Windows.PdfViewer;assembly=Syncfusion.PdfViewer.WPF"
      
      xmlns:local="clr-namespace:SOSTeam.TravelAgency.WPF.Views.Owner"
      mc:Ignorable="d" 
      d:DesignHeight="662" d:DesignWidth="804"
      Title="PDFReportPage">

    <Grid>
        <Grid>
            <StackPanel>
                <pdfViewer:PdfViewerControl x:Name="PdfViewer" Zoom="75" Height="540"></pdfViewer:PdfViewerControl>
                <Grid Height="100">
                    <Button Content="Nazad" FontSize="25" Margin="580,20,0,20" Width="150" Height="50" Click="Button_Click" Style="{StaticResource OwnerButtons}" RenderTransformOrigin="0.5,0.5">
                        <Button.RenderTransform>
                            <ScaleTransform ScaleX="1" ScaleY="1" />
                        </Button.RenderTransform>

                    </Button>
                </Grid>
            </StackPanel>
        </Grid>
    </Grid>
</Page>
